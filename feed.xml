---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Michal Sakowicz</title>
  <link href="http://michalsakowicz.com/"/>
  <link type="application/atom+xml" rel="self" href="http://michalsakowicz.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://michalsakowicz.com/</id>
  <author>
    <name>Michal Sakowicz</name>
    <email>msakowicz.contact@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://michalsakowicz.com/{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="http://michalsakowicz.com/{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Michal Sakowicz</name>
      <uri>http://michalsakowicz.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>